    "#Problema 1 punto a (continuos)\n",
    "x=sym.Symbol('x')\n",
    "y=sym.Symbol('y')\n",
    "def f_xy(x,y):\n",
    "    f=(2/3)*(x+2*y)\n",
    "    return f\n",
    "es_fun_conjunta=False\n",
    "f=f_xy(x,y)\n",
    "if 1==sym.integrate(f,(x,0,1),(y,0,1)):\n",
    "    es_fun_conjunta=True\n",
    "es_fun_conjunta"
   ]
  }
